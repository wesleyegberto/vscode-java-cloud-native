{
	"name": "vscode-java-cloud-native",
	"publisher": "wesleyegberto",
	"displayName": "VSCode Java Cloud Native",
	"description": "Extension to help write Java application for Cloud",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/wesleyegberto/vscode-java-cloud-native.git"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:vscode-java-cloud-native.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "dockerfile",
				"path": "./snippets/docker/docker.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"package": "vsce package",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
